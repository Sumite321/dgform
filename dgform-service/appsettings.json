{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",

  "spring": {
    "application": {
      "name": "dgform-service"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8001/eureka/",
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": false
    },
    "instance": {
      "port": 8010,
      "isInstanceEnabledOnInit": true,
      "preferIpAddress": true
    }
  }


}
